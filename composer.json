{
    "name": "yiisoft/yii2-psr-log-source",
    "description": "A PSR LoggerInterface implementation that routes to the Yii logger",
    "keywords": [
        "yii2",
        "symfony",
        "loggerinterface",
        "psr-3"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2-psr-log-source/issues",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "ircs://irc.libera.chat:6697/yii",
        "source": "https://github.com/yiisoft/yii2-psr-log-source"
    },
    "authors": [
        {
            "name": "Sam Mousa",
            "email": "sam@mousa.nl"
        }
    ],
    "require": {
        "php": ">=8.1",
        "yiisoft/yii2": ">=2.0.4",
        "psr/log": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "symplify/easy-coding-standard": "^10.1",
        "vimeo/psalm": "^5",
        "captainhook/plugin-composer": "^5.3",
        "phpstan/phpstan": "^1.9",
        "ramsey/conventional-commits": "^1.3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "yii\\psr\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "vendor/yiisoft/yii2/Yii.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "provide": {
        "psr/log-implementation": "3.0"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "captainhook/plugin-composer": true
        }
    }
}
